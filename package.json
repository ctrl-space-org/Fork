{
  "name": "fork",
  "version": "0.0.1",
  "description": "API Gateway e API Manager",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch \"src/server/\" --exec \"sucrase-node src/server/init.ts\" -e ts",
    "devts": "nodemon --watch \"src/server/\" --exec \"ts-node src/server/init.ts\" -e ts",
    "test": "jest --no-cache",
    "build": "tsc",
    "prod": "npm run build && node dist/server/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sombratwo/Fork.git"
  },
  "keywords": [
    "API",
    "Gateway",
    "API",
    "Manager"
  ],
  "author": "Leonardo Lucas dos Santos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sombratwo/Fork/issues"
  },
  "homepage": "https://github.com/sombratwo/Fork#readme",
  "dependencies": {
    "@types/dotenv": "^6.1.1"
  // "dotenv": "^7.0.0",
  },
  "devDependencies": {
    "@types/supertest": "^2.0.7",
    "@types/jest": "^24.0.11",
    "ts-jest": "^24.0.1",
    "sucrase": "^3.10.1",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
  // "@types/express": "^4.16.1",
  // "@types/mongoose": "^5.3.24",
  //  "@types/readline-sync": "^1.4.3",
    
